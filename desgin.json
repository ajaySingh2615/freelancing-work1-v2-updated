          "base": "1rem",
          "medium": "1.125rem",
          "large": "1.5rem",
          "xlarge": "2.25rem",
          "xxlarge": "3rem"
        },
        "fontWeights": {
          "regular": 400,
          "medium": 500,
          "semiBold": 600,
          "bold": 700
        },
        "lineHeight": {
          "tight": 1.2,
          "normal": 1.6,
          "loose": 2
