      "transitions": {
        "default": "all 0.3s ease",
        "fast": "all 0.2s ease",
        "slow": "all 0.5s ease"
      },
      "breakpoints": {
        "xs": "23.4375rem",
        "sm": "36rem",
        "md": "48rem",
        "lg": "62rem",
        "xl": "75rem"
      },
      "zIndex": {
        "dropdown": 1000,
        "sticky": 1020,
        "fixed": 1030,
        "modal": 1050,
        "popover": 1060,
        "tooltip": 1070
      }
    },
