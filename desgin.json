        }
      },
      "spacing": {
        "xs": "0.3125rem",
        "sm": "0.625rem",
        "md": "0.9375rem",
        "lg": "1.25rem",
        "xl": "1.875rem",
        "xxl": "2.5rem",
        "section": "5rem"
      },
      "borders": {
        "radius": {
          "small": "0.25rem",
          "medium": "0.5rem",
          "large": "0.75rem",
          "pill": "3.125rem",
          "circle": "50%"
        },
        "width": {
          "thin": "0.0625rem",
          "medium": "0.125rem",
          "thick": "0.1875rem"
        },
        "style": "solid"
