    "interactions": {
      "navigation": {
        "hover": {
          "effect": "color change and underline",
          "transition": "all 0.3s ease"
        },
        "sticky": {
          "trigger": "scroll position > 100px",
          "effect": "header becomes sticky with box shadow",
          "animation": "fadeInDown 0.5s ease"
        },
        "mobile": {
          "trigger": "hamburger button click",
          "effect": "slide in menu from right",
          "animation": "slideInRight 0.3s ease"
        }
      },
      "buttons": {
        "hover": {
          "effect": "color and background change",
          "transition": "all 0.3s ease"
        },
        "active": {
          "effect": "slight scale down",
          "transition": "transform 0.2s ease"
        }
      },
      "cards": {
        "hover": {
          "effect": "translateY(-0.3125rem) and increased shadow",
          "transition": "all 0.3s ease"
        }
      },
      "backToTop": {
        "show": {
          "trigger": "scroll position > 200px",
          "effect": "fade in and become visible",
          "transition": "all 0.3s ease"
        },
        "click": {
          "effect": "smooth scroll to top",
          "animation": "scrollTop 800ms ease"
        }
      },
      "search": {
        "open": {
          "trigger": "search icon click",
          "effect": "fade in overlay with search input",
          "animation": "fadeIn 0.3s ease"
        },
