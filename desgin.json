      "shadows": {
        "small": "0 0.125rem 0.3125rem rgba(0, 0, 0, 0.1)",
        "medium": "0 0.25rem 0.625rem rgba(0, 0, 0, 0.1)",
        "large": "0 0.5rem 1.25rem rgba(0, 0, 0, 0.1)"
