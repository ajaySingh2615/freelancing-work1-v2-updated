        "primary": {
          "main": "#003585",
          "dark": "#002a6a",
          "light": "#1a4e9e"
        },
        "accent": {
          "main": "#FEBA02",
          "dark": "#dba002",
          "light": "#ffc42f"
        },
        "secondary": {
          "main": "#149DE1",
          "dark": "#0f7fb8",
          "light": "#3eb1eb"
        },
        "text": {
          "primary": "#333333",
          "secondary": "#777777",
          "light": "#ffffff"
        },
        "background": {
